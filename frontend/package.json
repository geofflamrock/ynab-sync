{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "pnpm run generate:css -- --minify",
    "build:dev": "pnpm run prisma:generate",
    "build:remix": "remix build",
    "build:worker": "tsc ./worker/sync.ts --outDir ./worker/build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "pnpm run generate:css --watch",
    "dev:remix": "remix dev",
    "dev:worker": "pnpm run build:worker && node ./worker/build/worker/sync.js",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "prisma:generate": "prisma generate",
    "generate:css": "tailwindcss -o ./app/tailwind.css",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.12",
    "@prisma/client": "^4.5.0",
    "@remix-run/node": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "@remix-run/serve": "^1.7.2",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "isbot": "^3.5.4",
    "lodash": "4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.1",
    "ynab-sync-st-george-au": "0.3.2",
    "ynab-sync-westpac-au": "0.4.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.2",
    "@remix-run/eslint-config": "^1.7.2",
    "@tailwindcss/forms": "^0.5.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "8.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "prisma": "^4.5.0",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  }
}
